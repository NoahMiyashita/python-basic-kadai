      "source": [
        "# 消費税込みの金額を計算する関数\n",
        "def calculate_tax_included(price, tax_rate):\n",
        "    return price * (1 + tax_rate / 100)\n",
        "\n",
        "# 金額110円、消費税10%\n",
        "price = 110\n",
        "tax_rate = 10\n",
        "\n",
        "# 関数を使って税込価格を計算\n",
        "total_price = calculate_tax_included(price, tax_rate)\n",
        "\n",
        "# 結果を表示\n",
        "print(\"税込価格は\", total_price, \"円です。\")\n"
      ]
    }
